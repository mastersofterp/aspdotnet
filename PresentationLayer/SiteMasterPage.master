        <script>

            var TimeNow =new Date();//new Date().getTime()
            //$('#ctl00_hdfMaintenanceTime').val()
            if( '<%= Session["miantenanceSTime"].ToString() %>'=="null")
                console.log("Start Date Not Define")
            else
            {
                var startTime = '<%= Session["miantenanceSTime"].ToString() %>' ;//$('#ctl00_hdfMaintenanceTime').val();

                var dateParts = startTime.match(/(\d{2})\/(\d{2})\/(\d{4}) (\d{2}):(\d{2}):(\d{2})/);
                var stime = new Date(dateParts[3], dateParts[2] - 1, dateParts[1], dateParts[4], dateParts[5], dateParts[6]);
                console.log('stime='+stime);
                var ShowOnTime = new Date(dateParts[3], dateParts[2] - 1, dateParts[1], dateParts[4], dateParts[5], dateParts[6]);
                console.log('show ON time='+ShowOnTime);
                //$('#ctl00_hdfMaintenanceFlag').val()

                var mainFlag = '<%=Session["maintenanceFlag"].ToString()%>'
                if( stime <=TimeNow && mainFlag=="0" )
                {
                    // alert("stime="+stime+"timenow="+TimeNow);
                    window.location.href="<%=Page.ResolveUrl("~/UnderConstruction.aspx")%>";//Server.MapPath("~/UnderConstruction.aspx");
                }
                else{

                    var beforeTime = new Date(stime.setHours(stime.getHours() - 1));
                    console.log('beforeTime='+beforeTime);
                    var AlertTimeDiff= $('#ctl00_hdfAlerFreq').val();//60000; 
                    // console.log("alert="+AlertTimeDiff);

                    // console.log("beforeTime="+beforeTime);
                    if(mainFlag=="0" && beforeTime<=TimeNow)
                    {
                        window.setInterval(function () 
                        {
                            debugger;
                            //showNotification(stime);
                            Swal.fire({
                                icon: 'info',
                                width: 300,
                                padding: '1em',
                                title: '<span style="font-size: 15px;">Your Web Portal Will Goes in Maintenence On '+ShowOnTime+'</span>',
                                showConfirmButton: true,
                                allowOutsideClick: false,
                                allowEscapeKey: false,
                                //timer: 5000
                            })
                            //alert("Your Web Portal Will Goes in Maintenence At "+stime+"");

                        },AlertTimeDiff );
                    }
                }
            }
        </script>
