        <%--===== Theme Background Script added by gaurav k 24012023 =====--%>
        <script>
            var els = document.getElementsByClassName("styled-button");

            for (var i = 0; i < els.length; i++) {
                // console.log(els[i].id);
                els[i].addEventListener("click", function (event) {
                    changeBackground(this.id);
                });
            }

            function changeBackground(id) {
                // document.querySelectorAll('[id^="background"]');
                // var classes = console.log(document.querySelector("body").classList.length);
                // clear all background classes from body
              const cls = [
                "background-theme-defaultthem",
                "background-theme-raindrop",
                "background-theme-seastone",
                "background-theme-seawaves",
                "background-theme-hillfogg",
                "background-theme-waterdrop"
              ];

                document.querySelector("body").classList.remove(...cls);

                //if(id ==  "defaultthem"){
                //    document.querySelector("body").classList.add("background-theme-defaultthem");
                //    localStorage.setItem("theme", "background-theme-defaultthem");
                //}
                //else if(id ==  "raindrop"){
                //    document.querySelector("body").classList.add("background-theme-raindrop");
                //    document.querySelector("body").classList.add("bg-theme-gif");
                //    localStorage.setItem("theme", "background-theme-raindrop");
                //}
                switch (id) {
                    case "defaultthem":
                        document.querySelector("body").classList.add("background-theme-defaultthem");
                        localStorage.setItem("theme", "background-theme-defaultthem");
                        document.querySelector("body").classList.remove("bg-theme-gif");
                        break;
                    case "raindrop":
                        document.querySelector("body").classList.add("background-theme-raindrop");
                        document.querySelector("body").classList.add("bg-theme-gif");
                        localStorage.setItem("theme", "background-theme-raindrop");
                        break;
                    case "seastone":
                        document.querySelector("body").classList.add("background-theme-seastone");
                        document.querySelector("body").classList.add("bg-theme-gif");
                        localStorage.setItem("theme", "background-theme-seastone");
                        break;
                    case "seawaves":
                        document.querySelector("body").classList.add("background-theme-seawaves");
                        document.querySelector("body").classList.add("bg-theme-gif");
                        localStorage.setItem("theme", "background-theme-seawaves");
                        break;
                    case "hillfogg":
                        document.querySelector("body").classList.add("background-theme-hillfogg");
                        document.querySelector("body").classList.add("bg-theme-gif");
                        localStorage.setItem("theme", "background-theme-hillfogg");
                        break;
                    case "waterdrop":
                        document.querySelector("body").classList.add("background-theme-waterdrop");
                        document.querySelector("body").classList.add("bg-theme-gif");
                        localStorage.setItem("theme", "background-theme-waterdrop");
                        break;
                }
            }
            
            //----------- Local Storage of theme background theme ------------//
            if (localStorage.getItem("theme") == "background-theme-defaultthem") { 
                changeBackground("defaultthem");
            }
            else if (localStorage.getItem("theme") == "background-theme-raindrop") { 
                changeBackground("raindrop");
            }
            else if (localStorage.getItem("theme") == "background-theme-seastone") { 
                changeBackground("seastone");
            }
            else if (localStorage.getItem("theme") == "background-theme-seawaves") { 
                changeBackground("seawaves");
            }
            else if (localStorage.getItem("theme") == "background-theme-hillfogg") { 
                changeBackground("hillfogg");
            }
            else if (localStorage.getItem("theme") == "background-theme-waterdrop") { 
                changeBackground("waterdrop");
            }

        </script>
