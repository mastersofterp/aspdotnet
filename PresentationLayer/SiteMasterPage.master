        <%--shortcut-key Logic added by Swapnil - 25-11-21--%>
        <script>

            var arrSC = new Array();
            var shortcut = "";

            function callUrl(shortcut) {
                $.ajax({
                    type: "POST",
                    url: '<%=Page.ResolveUrl("~/PageBasedWebMethod.aspx/GetShortCut")%>',
                    data: '{sc:"' + shortcut + '"}',
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (d) {
                        //debugger
                        var data = JSON.parse(d.d);
                        var PageNo;
                        $.each(data, function (a, b) {
                            PageNo = b.PageNo;
                        });

                        if (PageNo.length)
                            window.location.href = $("a[href$='pageno=" + PageNo + "']").attr('href')
                        else
                            alert('No shortcut is defined.');
                    },
                    failure: function (response) {
                        alert("Err1");
                    },
                    error: function (response) {
                        alert(response.responseText);
                    }
                });
            }

            $(document).ready(function () {
                $(document).on("keyup", function (e) {
                    if (e.shiftKey) {
                        arrSC.push(String.fromCharCode(e.which));
                    }
                    else if (e.which == 16) {
                        if (arrSC[0] == "6") {
                            shortcut = "";
                            for (var i = 1; i < arrSC.length; i++) {
                                shortcut += arrSC[i];
                            }
                            arrSC = [];
                            callUrl(shortcut);
                        }
                        else {
                            arrSC = [];
                        }
                    }
                });

