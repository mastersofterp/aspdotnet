        <%--Delete Favorite Links From Nav Right--%>
        <script>
            function DeleteFavLinks(ele) {

                var UA_No = '<%=Session["userno"] %>';
                var link = $(ele).prev().attr("href");

                var LinkNo = link.match("pageno=(.*)&");
                if (LinkNo == null) {
                    LinkNo = link.substring(link.lastIndexOf("=") + 1, link.length);
                }
                else {
                    LinkNo = LinkNo[1];
                }
                //Remove Star from Links
                $('li.dynamic').find('.fa').each(function (z, objz) {
                    var link_d = $(this).prev().attr('href');

                    var LinkNo_d = link_d.match("pageno=(.*)&");
                    if (LinkNo_d == null) {
                        LinkNo_d = link_d.substring(link_d.lastIndexOf("=") + 1, link_d.length);
                    }
                    else {
                        LinkNo_d = LinkNo_d[1];
                    }
                    //Compairing Page No's
                    if (LinkNo_d == LinkNo) {
                        $(this).removeClass('active');
                    }
                });
                //End

                //Delete Links
                $.ajax({
                    url: '<%=Page.ResolveUrl("~/WEB_API/Add_Fav_Links.asmx/Add_FavLink")%>',
                    data: { UA_NO: UA_No, LINK_NO: LinkNo, OTYPE: 2 },
                    type: 'POST',
                    dataType: 'text',
                    success: function (r) {
                        var json = "{" + r.split("{")[1].split("}")[0] + "}";
                        var obj = jQuery.parseJSON(json);
                    },
                    error: function (err) {
                        alert('3Something went Wrong !!!');
                    }
                });
                //End

                //Get Links
                var UL_Length = $(ele).closest('ul').children().length;
                if (UL_Length == 1) {
                    $(ele).closest('ul').prev().closest('li').remove();
                    $(ele).closest('ul').prev().remove();
                    if ($('#DynamicDiv ul').length == 1) {
                        $('#ctl00_divFavorite').html('<span><center><h3 style="font-family: ptsans, sans-serif;margin-top: 50%;color:#3498DB"><i class="fa fa-star-half-o" aria-hidden="true"></i><br>No Favorite Links Added !!</h1></center></span>');
                    }
                }
                else {
                    $(ele).parent().remove();
                }
                //End
            }
        </script>
