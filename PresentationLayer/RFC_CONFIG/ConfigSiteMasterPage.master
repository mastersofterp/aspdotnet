        <script type="text/javascript">
            //jQuery(function ($) {

            var OffsetDropdown = function () {
                var dropdown = $('.open>.dropdown-menu')

                if (dropdown.length == 0)
                    return

                var rightOffset = dropdown.offset().css('left', '0px') + dropdown.width()
                var browserWidth = $('body').innerWidth()
                var neededLeftOffset = dropdown.position().css('left', '0px') - (rightOffset - browserWidth)

                if (neededLeftOffset < 0) {
                    dropdown.css({ left: neededLeftOffset - 3 })
                } else {
                    dropdown.css({ left: 0 })
                }
            }
            // });
        </script>

        <%-- Search box --%>
        <script type="text/javascript">
            $(document).ready(function ($) {
                $('.jj #ctl00_mainMenu ul li').css({ 'display': 'none' });
                $('.live-search-list #ctl00_mainMenu').clone().appendTo('.jj');

                $('.jj #ctl00_mainMenu').find('li').each(function () {
                    $(this).attr('data-search-term', $(this).text().toLowerCase());
                });

                $('body').on('keyup', '.live-search-box', function () {

                    var searchTerm = $(this).val().toLowerCase();
                    $('.jj').find('li').each(function () {

                        if ($(this).filter('[data-search-term *= ' + searchTerm + ']').length > 0 || searchTerm.length < 1) {
                            $(this).fadeIn();
                        } else {
                            $(this).fadeOut();
                        }
                        return 0;
                    });

                    if ($('.live-search-box').val() == '') {
                        $('.live-search-area').hide();
                        $(this).removeClass('width210');
                        $('.cross-btn').fadeOut();

                    } else {
                        $('.live-search-area').show();
                        $(this).addClass('width210');
                        $('.cross-btn').fadeIn();
                    }
                });

                $(".navbar-toggler").click(function () {
                    $(".live-search-box").fadeToggle();
                    $('.cross-btn, .live-search-area').fadeOut();
                });

                $('body').on('click', '.cross-btn', function () {
                    $('.search-box .live-search-area').hide();
                    $('.live-search-box').val('');
                    $('.live-search-box').removeClass('width210');
                    $('.cross-btn').fadeOut();
                });
            });

        </script>
