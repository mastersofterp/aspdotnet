        <script type="text/javascript">
            $(document).ready(function () {
                $('a:not(.popout).dynamic').parent('li').css({ 'display': 'flex' });
                $('a:not(.popout).dynamic').parent('li').append('<i class="fa fa-star"></i>');
                var UA_No = '<%=Session["userno"] %>';
                //-------------------------------------------------------------------

                //----------------------------------------------------------------------

                $('li.dynamic').find('.fa').click(function () {

                    $(this).toggleClass('active');
                    var link = $(this).prev().attr('href');

                    var LinkNo = link.match("pageno=(.*)&");
                    if (LinkNo == null) {
                        LinkNo = link.substring(link.lastIndexOf("=") + 1, link.length);
                    }
                    else {
                        LinkNo = LinkNo[1];
                    }
                    var col = $(this).css("color");
                    var color = '';
                    //Find Color of Active Element
                    hexc(col);
                    //Web Api Call
                    if (color == '#e60000') {
                        $.ajax({
                            url: '<%=Page.ResolveUrl("~/WEB_API/Add_Fav_Links.asmx/Add_FavLink")%>',
                            data: { UA_NO: UA_No, LINK_NO: LinkNo, OTYPE: 1 },
                            type: 'POST',
                            dataType: 'text',
                            success: function (r) {
                                var json = "{" + r.split("{")[1].split("}")[0] + "}";
                                var obj = jQuery.parseJSON(json);
                            },
                            error: function (err) {
                                alert('1Something went Wrong !!!');
                            }
                        });
                    }
                    else if (color == '#a6a6a6') {
                        $.ajax({
                            url: '<%=Page.ResolveUrl("~/WEB_API/Add_Fav_Links.asmx/Add_FavLink")%>',
                            data: { UA_NO: UA_No, LINK_NO: LinkNo, OTYPE: 2 },
                            type: 'POST',
                            dataType: 'text',
                            success: function (r) {
                                var json = "{" + r.split("{")[1].split("}")[0] + "}";
                                var obj = jQuery.parseJSON(json);
                            },
                            error: function (err) {
                                alert('2Something went Wrong !!!');
                            }
                        });
                        }
                    // RGB to HEX Converter
                    function hexc(colorval) {
                        var parts = colorval.match(/^rgb\((\d+),\s*(\d+),\s*(\d+)\)$/);
                        delete (parts[0]);
                        for (var i = 1; i <= 3; ++i) {
                            parts[i] = parseInt(parts[i]).toString(16);
                            if (parts[i].length == 1) parts[i] = '0' + parts[i];
                        }
                        color = '#' + parts.join('');
                    }
                    //end
                });
            });
        </script>
